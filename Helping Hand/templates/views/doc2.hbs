            </nav>
        </div>
    </div>

    <!-- Head -->
    
    <section class="docs-head bg-primary py-3">
     
        <div class="container grid">
            <div>
                <h1 class="xl">volunteer</h1>
            </div>
            <img src="/images/helping hand.svg" alt="">
        </div>
    </section>

    <!-- Sub head -->
    <section class="features-sub-head bg-light py-3">
        <div id="you" class="container grid">
            <div>
                <h1 class="md">Want to become a Helping Hand for someone?</h1>
                <p>
                    If you wish to contribute to the society by helping any elderly people you are at the right place. If you are able to help at any of the following locations, you are welcome. Let's become a Helping Hand to make this society a better world to live.
                </p>
            </div>
        </div>
    </section>

    <div class="hello">



        
{{#each userinfo}}
        <form action="/doc2" method="DELETE" id="myForm">
        <div class="hello-item">
          <div class="card">
            <div class="card-content">
              <h2 class="card-header">{{name}}</h2>
              <h3><a class="locationbtn" href="{{locate}}">location</a></h3>
              <h3>{{gender}}</h3>
              <h3>{{number}}</h3>
              <p class="card-text">
                {{helptext}}
              </p>
              <input type="submit" class="card-btn" value="Accept->">
            </div>
          </div>
        </div>
        </form>
{{/each}}
        


      </div>
    
